# System identity (auto-detected by setup.sh)
PUID=1000
PGID=1000

# Timezone
TZ=America/New_York

# ProtonVPN WireGuard
WIREGUARD_PRIVATE_KEY=your_wireguard_private_key_here
VPN_SERVER_COUNTRY=Netherlands

# Plex claim token (get from https://plex.tv/claim)
PLEX_CLAIM_TOKEN=claim-xxxxxxxxxxxxxxxxxxxx

# Arr API keys (found in each app under Settings > General > API Key after first launch)
SONARR_API_KEY=your_sonarr_api_key_here
RADARR_API_KEY=your_radarr_api_key_here
PROWLARR_API_KEY=your_prowlarr_api_key_here
SABNZBD_API_KEY=your_sabnzbd_api_key_here

# Tautulli API key (found in Tautulli > Settings > Web Interface > API Key)
TAUTULLI_API_KEY=your_tautulli_api_key_here

# Grafana
GRAFANA_ADMIN_USER=admin
GRAFANA_ADMIN_PASSWORD=changeme
