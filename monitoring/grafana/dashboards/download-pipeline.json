      "title": "Prowlarr - Total Grabs",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 8, "y": 14 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        { "expr": "prowlarr_indexer_grab_total", "legendFormat": "Grabs" }
