{
  "annotations": { "list": [] },
  "editable": true,
  "graphTooltip": 1,
  "links": [],
  "panels": [
    {
      "id": 100,
      "title": "SABnzbd",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "collapsed": false,
      "panels": []
    },
    {
      "id": 1,
      "title": "SABnzbd - Download Speed",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        { "expr": "sabnzbd_download_speed_bytes", "legendFormat": "Speed" }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "Bps",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "orange", "value": 1048576 },
              { "color": "green", "value": 10485760 }
            ]
          }
        },
        "overrides": []
      },
      "options": { "graphMode": "area" }
    },
    {
      "id": 2,
      "title": "SABnzbd - Queue Size",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 6, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        { "expr": "sabnzbd_queue_total", "legendFormat": "Items" }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 5 },
              { "color": "red", "value": 20 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "id": 3,
      "title": "SABnzbd - Remaining Data",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 10, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        { "expr": "sabnzbd_queue_remaining_bytes", "legendFormat": "Remaining" }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 5368709120 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "id": 4,
      "title": "SABnzbd - Status",
      "description": "Paused or downloading",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 14, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        { "expr": "sabnzbd_paused", "legendFormat": "Paused" }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            { "options": { "0": { "text": "Downloading", "color": "green" } }, "type": "value" },
            { "options": { "1": { "text": "Paused", "color": "orange" } }, "type": "value" }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 1 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "id": 5,
      "title": "SABnzbd - Total Downloaded",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        { "expr": "sabnzbd_downloaded_bytes", "legendFormat": "Total" }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "thresholds": { "mode": "absolute", "steps": [{ "color": "blue", "value": null }] }
        },
        "overrides": []
      }
    },
    {
      "id": 6,
      "title": "SABnzbd - Download Speed Over Time",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 5 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        { "expr": "sabnzbd_download_speed_bytes", "legendFormat": "Download Speed" }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "Bps",
          "min": 0,
          "custom": { "drawStyle": "line", "lineWidth": 2, "fillOpacity": 25, "showPoints": "never", "gradientMode": "scheme" },
          "color": { "mode": "continuous-blues" }
        },
        "overrides": []
      }
    },
    {
      "id": 7,
      "title": "SABnzbd - Queue & Remaining Over Time",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 5 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        { "expr": "sabnzbd_queue_total", "legendFormat": "Queue Items" },
        { "expr": "sabnzbd_queue_remaining_bytes", "legendFormat": "Remaining Data" }
      ],
      "fieldConfig": {
        "defaults": {
          "min": 0,
          "custom": { "drawStyle": "line", "lineWidth": 2, "fillOpacity": 15, "showPoints": "never" }
        },
        "overrides": [
          { "matcher": { "id": "byName", "options": "Remaining Data" }, "properties": [{ "id": "unit", "value": "bytes" }, { "id": "custom.axisPlacement", "value": "right" }] }
        ]
      }
    },
    {
      "id": 200,
      "title": "Prowlarr (Indexers)",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 13 },
      "collapsed": false,
      "panels": []
    },
    {
      "id": 8,
      "title": "Prowlarr - Total Indexers",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 0, "y": 14 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
