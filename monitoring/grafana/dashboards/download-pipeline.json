      "fieldConfig": {
        "defaults": {
          "thresholds": { "mode": "absolute", "steps": [{ "color": "blue", "value": null }] }
        },
        "overrides": []
      }
    },
    {
      "id": 11,
      "title": "Prowlarr - Total Queries",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 12, "y": 14 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        { "expr": "prowlarr_indexer_query_total", "legendFormat": "Queries" }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": { "mode": "absolute", "steps": [{ "color": "purple", "value": null }] }
        },
        "overrides": []
      }
    },
    {
      "id": 12,
      "title": "Prowlarr - Failed Grabs",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 16, "y": 14 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        { "expr": "prowlarr_indexer_grab_fail_total", "legendFormat": "Failed" }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 1 },
              { "color": "red", "value": 10 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "id": 13,
      "title": "Prowlarr - Failed Queries",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 20, "y": 14 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        { "expr": "prowlarr_indexer_query_fail_total", "legendFormat": "Failed" }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 1 },
              { "color": "red", "value": 10 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "id": 14,
      "title": "Prowlarr - Grabs Over Time",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 18 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        { "expr": "increase(prowlarr_indexer_grab_total[1h])", "legendFormat": "Grabs / hour" },
        { "expr": "increase(prowlarr_indexer_grab_fail_total[1h])", "legendFormat": "Failed / hour" }
      ],
      "fieldConfig": {
        "defaults": {
          "min": 0,
          "custom": { "drawStyle": "bars", "lineWidth": 1, "fillOpacity": 50, "showPoints": "never" }
        },
        "overrides": [
          { "matcher": { "id": "byName", "options": "Failed / hour" }, "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }] }
        ]
      }
    },
    {
      "id": 15,
      "title": "Prowlarr - Queries Over Time",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 18 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        { "expr": "increase(prowlarr_indexer_query_total[1h])", "legendFormat": "Queries / hour" },
        { "expr": "increase(prowlarr_indexer_query_fail_total[1h])", "legendFormat": "Failed / hour" }
      ],
      "fieldConfig": {
        "defaults": {
          "min": 0,
          "custom": { "drawStyle": "bars", "lineWidth": 1, "fillOpacity": 50, "showPoints": "never" }
        },
        "overrides": [
          { "matcher": { "id": "byName", "options": "Failed / hour" }, "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }] }
        ]
      }
    }
  ],
  "schemaVersion": 39,
  "templating": {
    "list": [
      {
        "name": "DS_PROMETHEUS",
        "type": "datasource",
        "query": "prometheus",
        "current": { "selected": true, "text": "Prometheus", "value": "prometheus" }
      }
    ]
  },
  "time": { "from": "now-24h", "to": "now" },
  "refresh": "1m",
