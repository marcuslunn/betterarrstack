      "targets": [
        { "expr": "prowlarr_indexer_enabled_total", "legendFormat": "Enabled" }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }] }
        },
        "overrides": []
      }
    },
