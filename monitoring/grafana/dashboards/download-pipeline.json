      "fieldConfig": {
        "defaults": {
          "thresholds": { "mode": "absolute", "steps": [{ "color": "blue", "value": null }] }
        },
        "overrides": []
      }
    },
    {
      "id": 9,
      "title": "Prowlarr - Enabled Indexers",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 4, "y": 14 },
